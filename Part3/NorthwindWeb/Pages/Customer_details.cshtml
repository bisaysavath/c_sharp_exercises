@page
@model NorthwindWeb.Pages.Customer_detailsModel

<div class="jumbotron">
    <h1 class="display-3">@Model.Customer.ContactName | @Model.Customer.ContactTitle</h1>
    <p class="lead">Title: @Model.Customer.ContactTitle</p>
    <p class="lead">Company: @Model.Customer.CompanyName</p>
    <p class="lead">Address: @Model.Customer.Address, @Model.Customer.City, @Model.Customer.Country @Model.Customer.PostalCode</p>
    <p class="lead">Phone: @Model.Customer.Phone</p>
</div>
